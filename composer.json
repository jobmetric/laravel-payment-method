{
    "name": "jobmetric/laravel-payment-method",
    "description": "This is a payment method management package for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "payment method",
        "payment",
        "management"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-payment-method",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.7",
        "jobmetric/laravel-extension": "^1.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\PaymentMethod\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\PaymentMethod\\PaymentMethodServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-payment-method/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-payment-method"
    }
}
